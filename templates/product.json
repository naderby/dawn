          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<a href=\"{{ product.vendor | url_for_vendor }}\"><img style=\"max-width:10rem; background:rgba(var(--color-foreground),.04);padding:1\nrem;border-radius:1\nrem;\" src=\"{{ product.vendor | prepend: \"vendor-\" | handle | append: \".svg\" | downcase | asset_url }}\"><\/a>"

          }
        },
        "description": {
          "type": "description",
          "settings": {
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        }
      },
      "block_order": [
        "b3823b51-5d12-4c31-a0c6-0c29676ea18d",
        "title",
        "subtitle",
        "price",
        "variant_picker",
        "9ee8db31-91e6-4b9d-8a1a-525454dd7b60",
        "quantity_selector",
        "buy_buttons",
        "vendor",
        "description",
        "share"
      ],
      "settings": {
        "enable_sticky_info": true,
        "hide_variants": false,
        "enable_video_looping": false
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
